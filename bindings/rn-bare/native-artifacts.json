[
  {
    "name": "ios-dist",
    "description": "Prebuilt static libraries and OpenSSL XCFramework for Maany MPC on iOS",
    "archive": "maany-mpc-ios-${version}.tar.gz",
    "envVar": "MAANY_MPC_IOS_ARCHIVE",
    "outputs": ["ios/dist"],
    "check": "ios/dist/device/libmaany_mpc_core.a"
  },
  {
    "name": "android-libs",
    "description": "Android static libraries for linking maany_mpc_rn",
    "archive": "maany-mpc-android-${version}.tar.gz",
    "envVar": "MAANY_MPC_ANDROID_ARCHIVE",
    "outputs": ["android/native-libs"],
    "check": "android/native-libs/arm64-v8a/libmaany_mpc_core.a"
  }
]
